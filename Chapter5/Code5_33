 w = 2  # simple assigment
 q = floor(tan(pi/2 - 0.1))
 f(x) = sin(w*x)
 sinc(x) = sin(pi*x)/(pi*x)  
 delta(t) = (t == 0)
 ramp(t) = (t  > 0) ? t : 0  # if t > 0 then t else 0 
 min(a,b) = (a <  b) ? a : b
 comb(n,k) = n!/(k!*(n-k)!)
 len3d(x,y,z) = sqrt(x**2 +y**2 + z**2)
 plot f(x) = sin(x*a), a = 0.2, f(x), a = 0.4, f(x)
 file = "mydata.dat" # string variable
 file(n) = sprintf("run_%d.dat",n) # string function
